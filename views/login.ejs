<%- include meta.ejs %>
<%- include header.ejs %>
<canvas id="Mycanvas" style=" background-color: #f7fafc;position: absolute;z-index: -1;overflow: hidden;"></canvas>
<div class="container summary">
    <%- include alert.ejs %>
    <div class="login-content">
        <h3 class="reg-title text-center">用户登录</h3>
        <form class="form-horizontal" method="post">
            <div class="form-group">
                <label for="useraccount" class="col-sm-2 control-label">用户账号</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="useraccount" name="useraccount" autocomplete="off">
                </div>
            </div>
            <div class="form-group">
                <label for="upwd" class="col-sm-2 control-label">登录密码</label>
                <div class="col-sm-10">
                    <input type="password" class="form-control" id="upwd" name="userpwd">
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button id="btn-login" type="submit" class="btn btn-primary">登录</button>
                </div>
            </div>
        </form>
    </div>
    <%- include footer.ejs %>
</div>
<script src="/js/plugin/bg.js"></script>